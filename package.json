{
	"name": "vsc-ttt",
	"displayName": "vsc-ttt",
	"publisher": "yoyuse",
	"repository": {
		"type": "git",
		"url": "https://github.com/yoyuse/vsc-ttt.git"
	},
	"icon": "images/icon.png",
	"description": "Yet another modeless Japanese input for VS Code",
	"version": "0.0.11",
	"engines": {
		"vscode": "^1.32.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:ttt.doTtt",
		"onCommand:ttt.doTttViaClipboard",
		"onCommand:ttt.doKkt"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "ttt.doTtt",
				"title": "Ttt: Do Ttt"
			},
			{
				"command": "ttt.doTttViaClipboard",
				"title": "Ttt: Do Ttt Via Clipboard"
			},
			{
				"command": "ttt.doKkt",
				"title": "Ttt: Kana Kanji Transliteration"
			}
		],
		"keybindings": [
			{
				"command": "ttt.doTtt",
				"key": "alt+j",
				"mac": "cmd+j",
				"when": "editorTextFocus"
			},
			{
				"command": "ttt.doTttViaClipboard",
				"key": "alt+j",
				"mac": "cmd+j",
				"when": "!editorTextFocus && inputFocus"
			},
			{
				"command": "ttt.doKkt",
				"key": "alt+shift+j",
				"mac": "cmd+shift+j",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/mocha": "^2.2.42",
		"@types/node": "^10.14.4",
		"@types/node-fetch": "^2.6.2",
		"tslint": "^5.16.0",
		"typescript": "^3.4.3",
		"vscode": "^1.1.28"
	},
	"dependencies": {
		"node-fetch": "^2.6.7"
	}
}
